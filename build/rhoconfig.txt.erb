start_path = '/app/Settings/init'
options_path = '/app/Settings'
app_db_version = <%= @rhoconfig[:app_db_version] %>

MinSeverity  = <%= @rhoconfig[:min_severity] %>
LogToOutput = 1
LogCategories = *
log_skip_post = 0
log_exclude_filter = 'login, password, pin'

MaxLogFileSize=<%= @rhoconfig[:max_log_file_size] %>

logserver = '<%= @rhoconfig[:log_server] %>' 
logname='InsiteMobile-rhodes'

KeepTrackOfLastVisitedPage = 0 
LastVisitedPage = ''
syncserver = <%= @rhoconfig[:sync_server] %>
no_ruby_threads=1

phonecall_duration_default_minutes = <%= @rhoconfig[:phonecall_duration_default_minutes] %>

#If the full_screen = 0, on android we need to add android:windowSoftInputMode="adjustPan" 
#to the platform/android/Rhodes/AndroidManifest.xml activity section.  This has been committed to our rhodes version 2.4 and 3.2
full_screen = 0
jqtouch_mode=1

splash_screen='zoom'
lead_cost_threshold = 100

quick_quote_url='<%= @rhoconfig[:quick_quote_url] %>'
resource_center_url='<%= @rhoconfig[:resource_center_url] %>'
resource_center_target= '/Pages/Home.aspx'
lead_forward_target= '/Sso/Pages/StrategyBaySso.aspx'
lead_forward_active_orders_target= '"/Sso/Pages/StrategyBaySso.aspx?Site=mobile"'
crm_url='<%= @rhoconfig[:crm_url] %>'



