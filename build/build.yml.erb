sdk: <%= @build_yml[:sdk_path] %>
name: <%= @build_yml[:name] %> 
version: <%= @build_yml[:version] %>
vendor: Insphere
build: <%= @build_yml[:build_type] %>
#app_type: rhoelements
capabilities:
  - non_motorola_device
  - native_browser
#motorola_license_company: <%= @build_yml[:motorola_license_company] %>
#motorola_license: <%= @build_yml[:motorola_license] %>
android: 
  package_name: <%= @build_yml[:package_name] %>
  URIScheme: <%= @build_yml[:uri_scheme] %>
  URIHost: <%= @build_yml[:uri_host] %>
  production:
    certificate: <%= @build_yml[:android_cert_path] %>
    password: "<%= @build_yml[:android_cert_password] %>"
    alias: "<%= @build_yml[:android_cert_alias] %>"
  #emulator: rhoandroid
  android_title: 0
  push:
    sender: <%= @build_yml[:android_push_sender] %>
    notifications: always
  extensions:
  - gcm-push
  capabilities: 
  - network_state
  - push
  - vibrate
  minSDK: "14"
  version: <%= @build_yml[:android_sdk_version] %>
applog: rholog.txt
encrypt_database: '<%= @build_yml[:encrypt_database] %>'
iphone:
  configuration: <%= @build_yml[:iphone_config] %>
  sdk: <%= @build_yml[:iphone_sdk] %>
  # ------------------ INSPHERE ENTERPRISE ACCOUNT -----------------------
  BundleIdentifier: <%= @build_yml[:bundle_identifier] %>
  codesignidentity: <%= @build_yml[:codesignidentity] %>
  provisionprofile: <%= @build_yml[:provisionprofile] %>
  # ----------------------------------------------------------------------
  entitlements:
  BundleURLScheme: <%= @build_yml[:bundle_url_scheme] %>
  capabilities:
  - push
  
extensions: <%= @build_yml[:extensions] %>