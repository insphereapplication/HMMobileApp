<div data-role="page" data-theme="b" data-cache="never">
	<%= offline_bar%>
		<div  data-theme="b" data-role="header">
			<a href="/app/Settings/index" data-direction="reverse" data-icon="back">Tools</a>
			<h1>Limit Counts <%= sync_spinner %></h1>
		</div>

		<div id="contentDiv" data-role="content" data-theme="b" >
			<ul data-role="listview" data-theme="d" data-inset="true">
				<li><h3>Opportunities</h3> <p> Using <%= Opportunity.open_opportunities_count %> of <%= AppInfo.instance.get_model_limits['Opportunity'] %></p></li> 
				<li><h3>Contacts</h3><p>  Using <%=Contact.count %> of <%= AppInfo.instance.get_model_limits['Contact'] %></p></li>
				<li><h3>Activities</h3><p> Using <%=Activity.count %> of <%= AppInfo.instance.get_model_limits['Activity'] %></p></li> 
				<li><h3>Policies</h3><p>  Using <%=Policy.count %> of <%= AppInfo.instance.get_model_limits['Policy'] %></p></li>
				<li><h3>Notes:</h3><p>  Using <%=Note.count %> of <%= AppInfo.instance.get_model_limits['Note'] %></p></li>
			</ul>
		</div>
</div>