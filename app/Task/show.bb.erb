<div id="pageTitle">
	<h1><%= @task.activity_type%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @task.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">ActivityType</td>
        <td class="itemValue"><%= @task.activity_type%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Subject</td>
        <td class="itemValue"><%= @task.subject%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">FollowUpDate</td>
        <td class="itemValue"><%= @task.follow_up_date%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">StartTime</td>
        <td class="itemValue"><%= @task.start_time%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EndTime</td>
        <td class="itemValue"><%= @task.end_time%></td>
      </tr>
      
    </table>
</div>