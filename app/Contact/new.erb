<div data-role="page" data-theme="b">
	<div data-inline="true">
		<%= offline_bar%>
		<div data-role="header" data-backbtn="false" data-theme="b">		
			<%= cancelbutton_by_origin(@params['origin']) %>
			<h1>Add Contact/Opp</h1>
			<div data-iconpos="right">
				<input type="submit" href="#" onclick="goBack()" data-theme="b" data-direction="reverse" value="Done" />
			</div>
		</div>
	</div>
	
	<div data-role="content">
		
		<div align="center">
			<label class="fieldLabel"><h3>Add Contact</h3></label>
		</div>

	
		<div data-role="collapsible" data-collapsed="false" >
			<h3>
				<a href="#" data-theme="b">General Information</a>
			</h3>
					
			<div data-role="fieldcontain">
				<div>
	        		<label for="contact[name]" class="fieldLabel">First Name:<font color="red">  *</font></label>
	        		<input maxlength="50" type="text" name="contact[name]" <%= placeholder( "First Name" ) %> />
				</div>
			</div>		

			<div data-role="fieldcontain">
				<div>
	        		<label for="contact[name]" class="fieldLabel">Last Name:<font color="red">  *</font></label>
	        		<input maxlength="50" type="text" name="contact[last_name]" <%= placeholder( "Last Name" ) %> />
				</div>
			</div>		

			<div data-role="fieldcontain">
				<div>
	        		<label for="contact[name]" class="fieldLabel">Email Address:<font color="blue">  +</font></label>
	        		<input maxlength="50" type="text" name="contact[name]" <%= placeholder( "Email Address" ) %> />
				</div>
			</div>		
		
			<div data-role="fieldcontain">
				<div>
	            	<label for="contact[mobilephone]" class="fieldLabel">Mobile Phone:<font color="blue">  +</font></label>
					<input maxlength="50" type="text" name="contact[telephone1]" <%= placeholder( "Mobile Phone" ) %> />
		    	</div>
			</div>
				
			<div data-role="fieldcontain">
				<div>
	            	<label for="contact[mobilephone]" class="fieldLabel">Business Phone:<font color="blue">  +</font></label>
					<input maxlength="50" type="text" name="contact[telephone1]" <%= placeholder( "Business Phone" ) %> />
		    	</div>
			</div>
				
			<div data-role="fieldcontain">
				<div>
	            	<label for="contact[mobilephone]" class="fieldLabel">Home Phone:<font color="blue">  +</font></label>
					<input maxlength="50" type="text" name="contact[telephone1]" <%= placeholder( "Home Phone" ) %> />
		    	</div>
			</div>

			<div data-role="fieldcontain">
				<div>
	            	<label for="contact[mobilephone]" class="fieldLabel">Alternative Phone:<font color="blue">  +</font></label>
					<input maxlength="50" type="text" name="contact[telephone1]" <%= placeholder( "Alternative Phone" ) %> />
		    	</div>
			</div>

			<div data-role="fieldcontain">
				<div>
	            	<label for="contact[mobilephone]" class="select">Preferred Phone:</label>
					<div>
						<select name="contact[cssi_preferredphone]" tabindex="-1">
							<option selected="true" value="Business">Business</option>
							<option value="Mobile">Mobile</option>
							<option value="Home">Home</option>
							<option value="Alternate">Alternate</option>
						</select>
					</div>
		    	</div>
			</div>				

			<div data-role="fieldcontain">
				<div>
	            	<label for="contact[mobilephone]" class="select">Gender:</label>
					<div>
						<select name="contact[cssi_gender]" tabindex="-1">
							<option selected="true" value="1">Male</option>
							<option value="2">Female</option>
						</select>
					</div>
		    	</div>
			</div>				

			<div data-role="fieldcontain">
					<div>
		            	<label for="contact[mobilephone]" class="fieldLabel">Date of Birth:</label>
						<input maxlength="50" type="text" name="contact[telephone1]" <%= placeholder( "Alternative Phone" ) %> />
			    	</div>
			</div>				
		</div>
			

		<div data-role="collapsible" data-collapsed="false" >
			<h3>
				<a href="#" data-theme="b">Opportunity Details</a>
			</h3>
	
			<div data-role="fieldcontain">
