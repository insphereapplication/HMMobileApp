<div id="pageTitle">
	<h1><%= @contact.first_name%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @contact.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">FirstName</td>
        <td class="itemValue"><%= @contact.first_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">LastName</td>
        <td class="itemValue"><%= @contact.last_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">City</td>
        <td class="itemValue"><%= @contact.city%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">State</td>
        <td class="itemValue"><%= @contact.state%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Zip</td>
        <td class="itemValue"><%= @contact.zip%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Phone</td>
        <td class="itemValue"><%= @contact.phone%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Sex</td>
        <td class="itemValue"><%= @contact.sex%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">DateOfBirth</td>
        <td class="itemValue"><%= @contact.date_of_birth%></td>
      </tr>
      
    </table>
</div>